{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@contentful/gatsby-transformer-contentful-richtext": "13.1.0",
    "@nearform/gatsby-source-contentful": "^1.1.0",
    "@reach/router": "^1.2.1",
    "@svgr/webpack": "4.1.0",
    "await-exec": "^0.1.2",
    "babel-jest": "^24.1.0",
    "babel-plugin-styled-components": "^1.8.0",
    "contentful": "^7.1.0",
    "contentful-cli": "^0.20.0",
    "contentful-link-cleaner": "1.3.4",
    "dayjs": "^1.8.0",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "eslint-plugin-react-hooks": "1.4.0",
    "formik": "^1.4.1",
    "gatsby": "^2.0.83",
    "gatsby-plugin-google-tagmanager": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.10",
    "gatsby-plugin-offline": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-styled-components": "^3.0.3",
    "gatsby-plugin-svgr": "2.0.1",
    "gatsby-source-contentful": "^2.0.15",
    "gatsby-transformer-remark": "^2.1.12",
    "google-map-react": "1.1.2",
    "jest-styled-components": "^6.3.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "rc-slider": "^8.6.4",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-helmet": "^5.2.0",
    "react-inlinesvg": "0.8.4",
    "react-modal": "^3.8.1",
    "react-player": "1.9.3",
    "react-recaptcha": "^2.3.10",
    "react-tabs": "^3.0.0",
    "styled-components": "^4.1.1",
    "yup": "^0.26.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*test)|(.*(test|spec))\\.jsx?$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>",
      "__tests__"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^test-helpers(.*)$": "<rootDir>/__tests__/test-helpers$1"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "__tests__"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost:8000",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "refresh": "rm -rf .cache && gatsby develop",
    "format": "prettier --write '**/*.js'",
    "test": "jest",
    "test-watch": "jest --watch --verbose",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "./node_modules/.bin/eslint src && prettier --list-different '**/*.js'",
    "deploy": "yarn test && yarn lint && git push",
    "mock-deploy-build": "rm -rf ./node_modules/* && yarn install && yarn test && yarn lint && gatsby build",
    "netlify-deploy-current": "netlify deploy --dir=public",
    "setup": "yarn install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-jest": "2.1.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-cli": "^2.4.4",
    "gatsby-plugin-eslint": "^2.0.1",
    "gatsby-plugin-netlify": "^2.0.5",
    "gatsby-plugin-root-import": "^2.0.4",
    "husky": "1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-watch-typeahead": "0.2.1",
    "lint-staged": "8.1.5",
    "netlify-cli": "^2.1.3",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "^6.0.0",
    "webpack": "^4.27.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
