{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@contentful/gatsby-transformer-contentful-richtext": "12.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "gatsby": "^2.0.55",
    "gatsby-plugin-google-tagmanager": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.10",
    "gatsby-plugin-offline": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-react-svg": "^2.0.0",
    "gatsby-plugin-styled-components": "^3.0.3",
    "gatsby-source-contentful": "^2.0.15",
    "gatsby-transformer-remark": "^2.1.12",
    "jest-styled-components": "^6.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*test)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost:8000",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write '**/*.js'",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "./node_modules/.bin/eslint src",
    "deploy": "yarn test && yarn lint && git push",
    "mock-deploy-build": "rm -rf ./node_modules/* && yarn install && yarn test && yarn lint && gatsby build",
    "netlify-deploy-current": "netlify deploy --dir=public",
    "setup": "yarn install"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-cli": "^2.4.4",
    "gatsby-plugin-eslint": "^2.0.1",
    "gatsby-plugin-netlify": "^2.0.5",
    "gatsby-plugin-root-import": "^2.0.4",
    "jest": "^23.6.0",
    "netlify-cli": "^2.1.3",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.3",
    "webpack": "^4.27.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
